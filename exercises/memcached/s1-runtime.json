{
  "target": "bmv2",
  "p4info": "build/memcached.p4.p4info.txt",
  "bmv2_json": "build/memcached.json",
  "table_entries": [
    {
      "table": "MyIngress.ipv4_lpm",
      "default_action": true,
      "action_name": "MyIngress.drop",
      "action_params": { }
    },
    {
      "table": "MyIngress.response_ipv4_lpm",
      "match": {
        "hdr.ipv4.srcAddr": ["10.0.2.2", 32]
      },
      "action_name": "MyIngress.rewrite_ipv4_src",
      "action_params": {
        "srcAddr": "10.0.0.1"
      }
    },
    {
      "table": "MyIngress.response_ipv4_lpm",
      "match": {
        "hdr.ipv4.srcAddr": ["10.0.3.3", 32]
      },
      "action_name": "MyIngress.rewrite_ipv4_src",
      "action_params": {
        "srcAddr": "10.0.0.1"
      }
    },
    {
      "table": "MyIngress.memcached_request_exact",
      "default_action": true,
      "action_name": "MyIngress.rewrite_ipv4_dst",
      "action_params": {
        "dstAddr": "10.0.2.2"
      }
    },
    {
      "table": "MyIngress.ipv4_lpm",
      "match": {
        "hdr.ipv4.dstAddr": ["10.0.1.1", 32]
      },
      "action_name": "MyIngress.ipv4_forward",
      "action_params": {
        "dstAddr": "08:00:00:00:01:11",
        "port": 1
      }
    },
    {
      "table": "MyIngress.ipv4_lpm",
      "match": {
        "hdr.ipv4.dstAddr": ["10.0.2.2", 32]
      },
      "action_name": "MyIngress.ipv4_forward",
      "action_params": {
        "dstAddr": "08:00:00:00:02:00",
        "port": 2
      }
    },
    {
      "table": "MyIngress.ipv4_lpm",
      "match": {
        "hdr.ipv4.dstAddr": ["10.0.3.3", 32]
      },
      "action_name": "MyIngress.ipv4_forward",
      "action_params": {
        "dstAddr": "08:00:00:00:03:00",
        "port": 3
      }
    },
    {
      "table": "MyIngress.memcached_request_exact",
      "match": {
        "hdr.memcached_request.lastDigit": [49]
      },
      "action_name": "MyIngress.rewrite_ipv4_dst",
      "action_params": {
        "dstAddr": "10.0.3.3"
      }
    },
    {
      "table": "MyIngress.memcached_request_exact",
      "match": {
        "hdr.memcached_request.lastDigit": [51]
      },
      "action_name": "MyIngress.rewrite_ipv4_dst",
      "action_params": {
        "dstAddr": "10.0.3.3"
      }
    },
    {
      "table": "MyIngress.memcached_request_exact",
      "match": {
        "hdr.memcached_request.lastDigit": [53]
      },
      "action_name": "MyIngress.rewrite_ipv4_dst",
      "action_params": {
        "dstAddr": "10.0.3.3"
      }
    },
    {
      "table": "MyIngress.memcached_request_exact",
      "match": {
        "hdr.memcached_request.lastDigit": [55]
      },
      "action_name": "MyIngress.rewrite_ipv4_dst",
      "action_params": {
        "dstAddr": "10.0.3.3"
      }
    },
    {
      "table": "MyIngress.memcached_request_exact",
      "match": {
        "hdr.memcached_request.lastDigit": [57]
      },
      "action_name": "MyIngress.rewrite_ipv4_dst",
      "action_params": {
        "dstAddr": "10.0.3.3"
      }
    }
  ]
}
